import math
class Solution:
    def minEatingSpeed(self, piles: List[int], h: int) -> int:
        def total_hours(piles,hourly_rate):
            total=0
            for i in range(len(piles)):
                total=total+math.ceil(piles[i]/hourly_rate)
            return total
        low=1
        high=max(piles)
        ans=-1
        while low<=high:
            mid=(low+high)//2
            hours=total_hours(piles,mid)
            if hours<=h:
                ans=mid
                high=mid-1
            else:
                low=mid+1
        return ans
        
        

        
